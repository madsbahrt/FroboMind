<launch>

<group ns="fmExtractors"> 
    
    <node pkg="dynamic_reconfigure" name="param_load" type="dynparam" args="load /fmExtractors/row_finder $(find fmCore)/launch/hakotrac/rowconf.yaml"/>
    
    <node pkg="fmExtractors" name="row_finder" type="lidar_row_extractor">
        <param name="frame_id" value="laser_link"></param>
        <param name="subscriber_topic" value="/fmSensors/laser_msg"></param>
    </node>
    
</group>

<group ns="fmSensors">
    <node pkg="fmSensors" name="laser_range" type="lms111_node"  respawn="true" >
        <param name="host" value="192.168.0.11"/>
        <param name="topic_id" value="/fmSensors/laser_msg"/>
        <param name="frame_id" value="laser_link"/>
        <param name="invert_output" value="true" />
    </node>
</group>


</launch>